{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "paths": {
      "*": [
        "./frontend/src/*",
        "./frontend/test/*",
        "./enterprise/frontend/src/*",
        "./enterprise/frontend/test/*"
      ]
    },
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "frontend/src/**/*.ts",
    "frontend/src/**/*.tsx",
    "enterprise/frontend/src/**/*.ts",
    "enterprise/frontend/src/**/*.tsx",
    "frontend/test/**/*.ts",
    "frontend/test/**/*.tsx",
    "enterprise/frontend/test/**/*.ts",
    "enterprise/frontend/test/**/*.tsx"
, "frontend/src/minimal/components/YearlySales.jsx", "frontend/src/minimal/components/AnalyticsTrafficBySite.jsx", "frontend/src/minimal/components/BankingWidgetSummary.jsx", "frontend/src/minimal/components/GraphPie.jsx", "frontend/src/minimal/utils/Iconify.jsx", "frontend/src/minimal/utils/formatTime.js", "frontend/src/minimal/utils/formatNumber.js", "frontend/src/minimal/utils/cssStyles.js", "frontend/src/minimal/utils/createAvatar.js", "frontend/src/minimal/Iconify.jsx", "frontend/src/minimal/index.js", "frontend/src/minimal/index.jsx"  ],
  "exclude": ["node_modules"]
}
